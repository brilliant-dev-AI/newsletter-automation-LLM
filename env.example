# Newsletter Automation Environment Variables

# Skyvern AI API Configuration
SKYVERN_API_KEY=sk_api_your_actual_api_key_here
SKYVERN_API_URL=https://api.skyvern.com/v1
SKYVERN_TIMEOUT=30000

# Browserbase MCP Configuration  
BROWSERBASE_API_KEY=bb_api_your_actual_api_key_here
BROWSERBASE_PROJECT_ID=bb_proj_your_project_id_here
BROWSERBASE_API_URL=https://www.browserbase.com/api/v1

# OpenAI Configuration (for future LLM link extraction)
OPENAI_API_KEY=sk-your_actual_openai_api_key_here
OPENAI_MODEL=gpt-4

# AWS Configuration (for future email processing)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Email Configuration
NEWSLETTER_EMAIL=newsletter@yourdomain.com
NOTIFICATION_EMAIL=admin@yourdomain.com

# Application Configuration
NODE_ENV=development
PORT=3000
NEXT_PUBLIC_API_URL=http://localhost:3000
